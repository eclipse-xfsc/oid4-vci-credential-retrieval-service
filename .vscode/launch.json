    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {
                "CREDENTIALRETRIEVAL_OFFERING_TOPIC":"offering",
                "CREDENTIALRETRIEVAL_CASSANDRA_HOST": "127.0.0.1",
                "CREDENTIALRETRIEVAL_CASSANDRA_PORT": "9432",
